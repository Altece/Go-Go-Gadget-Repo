all: regserver regclient-test

regserver: regserver.6
	6l -o regserver regserver.6

regclient-test: regclient-test.6 regclient.6
	6l -o regclient-test regclient-test.6

regserver.6: regserver.go iregistry.6
	6g regserver.go

regclient-test.6: regclient-test.go regclient.6
	6g regclient-test.go

regclient.6: regclient.go iregistry.6
	6g regclient.go

iregistry.6: iregistry.go regcommon.go
	6g iregistry.go regcommon.go

clean:
	rm iregistry.6 regclient.6 regclient-test.6 regserver.6 regclient-test regserver